{
  "name": "blockchainvibe",
  "version": "1.0.0",
  "description": "BlockchainVibe - AI-powered blockchain news aggregator with Fetch.ai uAgents and SingularityNET MeTTa",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.5",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^10.1.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "remark-gfm": "^4.0.1",
    "styled-components": "^6.1.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "CI=false react-scripts build",
    "build:production": "CI=false REACT_APP_ENV=production react-scripts build",
    "build:ci": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy:pages": "npm run build:ci && wrangler pages deploy ./build --project-name=blockchainvibe",
    "deploy:worker": "cd server && wrangler deploy",
    "deploy:assets": "cd server && wrangler deploy --config assets-wrangler.toml"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": ["warn", { 
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^_",
        "ignoreRestSiblings": true
      }],
      "no-empty-pattern": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://blockchainvibe.news"
}
